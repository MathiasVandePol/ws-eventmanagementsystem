<?xml version="1.0" encoding="UTF-8"?>
<bpel:process exitOnStandardFault="yes" name="KdGLocatie.bpel"
    targetNamespace="http://ws.apache.org/axis2"
    xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:ns="http://ws.apache.org/axis2Artifacts" xmlns:ns0="http://ws.apache.org/axis2">
    <bpel:import importType="http://schemas.xmlsoap.org/wsdl/"
        location="LocatieService.wsdl" namespace="http://ws.apache.org/axis2"/>
    <bpel:import importType="http://schemas.xmlsoap.org/wsdl/"
        location="KdGLocatieArtifacts.wsdl" namespace="http://ws.apache.org/axis2Artifacts"/>
    <bpel:partnerLinks>
        <bpel:partnerLink myRole="LocatieServicePortTypeRole"
            name="bpelProcessPartner" partnerLinkType="ns:LocatieServicePortTypePLT"/>
    </bpel:partnerLinks>
    <bpel:variables>
        <bpel:variable element="ns0:annuleerReservering" name="annuleerReserveringRequest"/>
        <bpel:variable element="ns0:geefBeschikbareLocatie" name="geefBeschikbareLocatieRequest"/>
        <bpel:variable element="ns0:geefBeschikbareLocatieResponse" name="geefBeschikbareLocatieResponse"/>
    </bpel:variables>
    <bpel:sequence name="MainSequence">
        <bpel:pick createInstance="yes" name="SwitchInvokedOperation">
            <bpel:onMessage operation="annuleerReservering"
                partnerLink="bpelProcessPartner"
                portType="ns0:LocatieServicePortType" variable="annuleerReserveringRequest">
                <bpel:empty/>
            </bpel:onMessage>
            <bpel:onMessage operation="geefBeschikbareLocatie"
                partnerLink="bpelProcessPartner"
                portType="ns0:LocatieServicePortType" variable="geefBeschikbareLocatieRequest">
                <bpel:reply name="ReplyToGeefBeschikbareLocatie"
                    operation="geefBeschikbareLocatie"
                    partnerLink="bpelProcessPartner"
                    portType="ns0:LocatieServicePortType" variable="geefBeschikbareLocatieResponse"/>
            </bpel:onMessage>
        </bpel:pick>
    </bpel:sequence>
</bpel:process>
