<?xml version="1.0" encoding="UTF-8"?>
<bpel:process exitOnStandardFault="yes" name="KdGStudenten.bpel"
    targetNamespace="http://ws.apache.org/axis2"
    xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:ns="http://ws.apache.org/axis2Artifacts" xmlns:ns0="http://ws.apache.org/axis2">
    <bpel:import importType="http://schemas.xmlsoap.org/wsdl/"
        location="StudentenService.wsdl" namespace="http://ws.apache.org/axis2"/>
    <bpel:import importType="http://schemas.xmlsoap.org/wsdl/"
        location="KdGStudentenArtifacts.wsdl" namespace="http://ws.apache.org/axis2Artifacts"/>
    <bpel:partnerLinks>
        <bpel:partnerLink myRole="StudentenServicePortTypeRole"
            name="bpelProcessPartner" partnerLinkType="ns:StudentenServicePortTypePLT"/>
    </bpel:partnerLinks>
    <bpel:variables>
        <bpel:variable element="ns0:annuleerLes" name="annuleerLesRequest"/>
        <bpel:variable element="ns0:planLes" name="planLesRequest"/>
        <bpel:variable element="ns0:isEenGepastVoorstel" name="isEenGepastVoorstelRequest"/>
        <bpel:variable element="ns0:isEenGepastVoorstelResponse" name="isEenGepastVoorstelResponse"/>
    </bpel:variables>
    <bpel:sequence name="MainSequence">
        <bpel:pick createInstance="yes" name="SwitchInvokedOperation">
            <bpel:onMessage operation="annuleerLes"
                partnerLink="bpelProcessPartner"
                portType="ns0:StudentenServicePortType" variable="annuleerLesRequest">
                <bpel:empty/>
            </bpel:onMessage>
            <bpel:onMessage operation="planLes"
                partnerLink="bpelProcessPartner"
                portType="ns0:StudentenServicePortType" variable="planLesRequest">
                <bpel:empty/>
            </bpel:onMessage>
            <bpel:onMessage operation="isEenGepastVoorstel"
                partnerLink="bpelProcessPartner"
                portType="ns0:StudentenServicePortType" variable="isEenGepastVoorstelRequest">
                <bpel:reply name="ReplyToIsEenGepastVoorstel"
                    operation="isEenGepastVoorstel"
                    partnerLink="bpelProcessPartner"
                    portType="ns0:StudentenServicePortType" variable="isEenGepastVoorstelResponse"/>
            </bpel:onMessage>
        </bpel:pick>
    </bpel:sequence>
</bpel:process>
